<?xml version="1.0"?>
<package format="2">
  <name>alica_turtlesim</name>
  <version>1.0.0</version>
  <description>
    contains base file for alica_turtle behaviour which uses ALICA engine
  </description>
  <maintainer email="dale.koenig@rapyuta-robotics.com">Yu Okamoto</maintainer>
  <license>Commercial</license>

  <author>Yu Okamoto</author>


  <buildtool_depend>ament_cmake</buildtool_depend>

  <depend>alica_engine</depend>
  <depend>autodiff</depend>
  <depend>constraintsolver</depend>
  <exec_depend>alica_standard_library</exec_depend>
  <export>
    <build_type condition="$ROS_VERSION == 2">ament_cmake</build_type>
    <build_type condition="$ROS_VERSION == 1">catkin</build_type>
  </export>
</package>
