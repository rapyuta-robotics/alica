<?xml version="1.0"?>
<package format="2">
  <name>alica-turtlesim</name>
  <version>1.0.0</version>
  <description>
    contains base file for alica_turtle behaviour which uses ALICA engine
  </description>
  <maintainer email="yu.okamoto@rapyuta-robotics.com">Yu Okamoto</maintainer>
  <license>Commercial</license>

  <author>Yu Okamoto</author>


  <buildtool_depend>cmake</buildtool_depend>

  <export>
    <build_type>cmake</build_type>
  </export>

  <depend>alica_engine</depend>
  <depend>constraintsolver</depend>
  <depend>alica_solver_interface</depend>
  <depend>autodiff</depend>
  <depend>turtle_interface</depend>

</package>
