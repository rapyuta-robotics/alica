<package>
  <name>robot_control</name>
  <version>0.0.1</version>
  <description>robot_control provides a GUI plugin sending process_manager Commands.</description>
  <maintainer email="opfer@vs.uni-kassel.de">Stephan Opfer</maintainer>

  <license>MIT</license>

  <author>Stephan Opfer</author>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>message_generation</build_depend>
  <build_depend>std_msgs</build_depend>
  <build_depend>rqt_gui</build_depend>
  <build_depend>rqt_gui_cpp</build_depend>
  <build_depend>process_manager</build_depend>
  <build_depend>agent_id</build_depend>
  <build_depend>alica_client</build_depend>
  <build_depend>pm_control</build_depend>
  <build_depend>alica_msgs</build_depend>

  <run_depend>message_runtime</run_depend>
  <run_depend>std_msgs</run_depend>
  <run_depend>rqt_gui</run_depend>
  <run_depend>rqt_gui_cpp</run_depend>
  <run_depend>process_manager</run_depend>
  <run_depend>agent_id</run_depend>
  <run_depend>alica_client</run_depend>
  <run_depend>pm_control</run_depend>
  <run_depend>alica_msgs</run_depend>

  <export>
    <rqt_gui plugin="${prefix}/plugin.xml"/>
  </export>
</package>
