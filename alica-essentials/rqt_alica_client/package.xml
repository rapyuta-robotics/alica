<package>
  <name>rqt_alica_client</name>
  <version>0.0.1</version>
  <description>rqt_alica_client provides a GUI plugin for viewing alica information</description>
  <maintainer email="TSchellien@gmx.net">Tobias Schellien</maintainer>

  <license>MIT</license>

  <author>Tobias Schellien</author>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>rqt_gui</build_depend>
  <build_depend>rqt_gui_cpp</build_depend>
  
  <run_depend>rqt_gui</run_depend>
  <run_depend>rqt_gui_cpp</run_depend>

  <export>
    <rqt_gui plugin="${prefix}/plugin.xml"/>
  </export>
</package>
